  <%= form_for :user, url: {action: "create"} do |f| %>
      <%= f.label :username %>
      <%= f.text_field :username %><br>
      <%= f.label :email %>
      <%= f.email_field :email %><br>
      <%= f.label :password %>
      <%= f.password_field :password %>
      <%= f.submit "Register" %>
    <% end %>
